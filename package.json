{
  "name": "kings-league",
  "type": "module",
  "version": "1.0.0",
  "description": "Kings League Project",
  "main": "index.js",
  "scripts": {
    "astro": "astro",
    "build": "astro build",
    "dev:api": "wrangler dev api/index.ts",
    "dev": "astro dev",
    "preview": "astro preview",
    "publish:api": "wrangler publish api/index.ts",
    "scrape:mvp": "node scrapping/mvp.js",
    "start": "astro dev",
    "test": "vitest",
    "test:cov": "vitest run --coverage"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@astrojs/tailwind": "3.0.1",
    "astro": "2.0.10",
    "cheerio": "1.0.0-rc.12",
    "hono": "2.7.3",
    "tailwindcss": "3.2.6"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "3.19.0",
    "@types/node": "18.11.18",
    "eslint-plugin-astro": "0.23.0",
    "prettier": "2.8.3",
    "prettier-plugin-astro": "0.8.0",
    "standard": "17.0.0",
    "typescript": "4.9.4",
    "vitest": "0.27.2",
    "wrangler": "2.8.0"
  },
  "eslintConfig": {
    "extends": [
      "standard",
      "plugin:astro/recommended"
    ],
    "rules": {
      "no-tabs": "off",
      "indent": "off",
      "space-before-function-paren": "off"
    },
    "overrides": [
      {
        "files": [
          "*.astro"
        ],
        "parser": "astro-eslint-parser"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carlos8alb/kings-league-project.git"
  },
  "bugs": {
    "url": "https://github.com/carlos8alb/kings-league-project/issues"
  },
  "homepage": "https://github.com/carlos8alb/kings-league-project#readme"
}